{
    "name": "jamesmills/laravel-datadog",
    "description": "A simple package to wrap DataDog API for Timeseries metrics",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel",
        "datadog"
    ],
    "authors": [
        {
            "name": "James Mills",
            "email": "james@jamesmills.co.uk"
        }
    ],
    "require": {
        "php": ">=7",
        "laravel/framework": "5.8.* || ^6"
    },
    "autoload": {
        "psr-4": {
            "JamesMills\\LaravelDataDog\\": "src/"

        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JamesMills\\LaravelDataDog\\LaravelDataDogServiceProvider"
            ]
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.12"
    }
}
